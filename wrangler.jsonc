{
    "$schema": "node_modules/wrangler/config-schema.json",
    "name": "your-nextjs-app",
    "main": ".open-next/worker.js",
    "compatibility_date": "2026-01-01",
    "compatibility_flags": [
      "nodejs_compat",
       "global_fetch_strictly_public"
    ],
    "minify": true,
    "assets": {
      "binding": "ASSETS",
      "directory": ".open-next/assets"
    },
    "observability": {
      "enabled": true
    }
  }
  